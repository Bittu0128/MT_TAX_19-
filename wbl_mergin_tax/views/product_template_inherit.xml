<odoo>
    <record id="view_product_template_form_inherit_account" model="ir.ui.view">
        <field name="name">product.template.form.inherit.account</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="account.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_id']" position="after">
                    <field name="revenue_account" widget="many2one" />
            </xpath>
        </field>
    </record>
</odoo>
